<?xml version="1.0" encoding="UTF-8"?>
<dsol:experimentalFrame xmlns:dsol="https://simulation.tudelft.nl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <experiment>
    <name>Animation example</name> <!-- the name of the experiment -->
    <analyst>Peter H.M. Jacobs</analyst> <!-- the name of the analyst -->
    <model>
      <model-class>nl.tudelft.simulation.examples.dsol.animation.BallModel</model-class>
    </model>
    <replications>
      <replication description="replication 0">
        <stream name="default" seed="12" />
      </replication>
      <replication description="replication 1">
        <stream name="default" seed="12" />
      </replication>
    </replications>
    <treatment>
      <startTime>2008-12-01T14:25:58</startTime> <!-- the startTime of this treatment -->
      <timeUnit>MINUTE</timeUnit>
      <warmupPeriod unit="MINUTE">0</warmupPeriod>
      <runLength unit="HOUR">3</runLength>
    </treatment>
  </experiment>
</dsol:experimentalFrame>	  