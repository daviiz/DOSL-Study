<?xml version="1.0" encoding="UTF-8" ?>
<!-- Created by Alexander Verbraeck on Apr 30, 2014 -->
<gisbeans:map xmlns:gisbeans="http://www.javel.nl/gisbeans">

	<!-- We start with the global map attributes -->
	<name>beijing</name>
	<units>dd</units>
	<extent>
		<minX>115.637</minX>
		<minY>39.247</minY>
		<maxX>117.099</maxX>
		<maxY>40.408</maxY>
	</extent>
	<gisbeans:image>
		<backgroundColor>
			<r>255</r>
			<g>255</g>
			<b>255</b>
		</backgroundColor>
		<size>
			<height>600</height>
			<width>800</width>
		</size>
	</gisbeans:image>

	<!-- Now we get the layers -->
	<gisbeans:layer>
		<name>roads</name>
		<data>
			<shapeFile>/gis/beijing/roads.shp</shapeFile>
		</data>
		<color>
			<r>120</r>
			<g>120</g>
			<b>120</b>
		</color>
		<outlineColor>
			<r>120</r>
			<g>120</g>
			<b>120</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>buildings</name>
		<data>
			<shapeFile>/gis/beijing/buildings.shp</shapeFile>
		</data>
		<color>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</color>
		<outlineColor>
			<r>200</r>
			<g>200</g>
			<b>200</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>natural</name>
		<data>
			<shapeFile>/gis/beijing/natural.shp</shapeFile>
		</data>
		<color>
			<r>127</r>
			<g>255</g>
			<b>197</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>255</g>
			<b>0</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>railways</name>
		<data>
			<shapeFile>/gis/beijing/railways.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>19</g>
			<b>127</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>19</g>
			<b>127</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
	<gisbeans:layer>
		<name>waterways</name>
		<data>
			<shapeFile>/gis/beijing/waterways.shp</shapeFile>
		</data>
		<color>
			<r>0</r>
			<g>148</g>
			<b>255</b>
		</color>
		<outlineColor>
			<r>0</r>
			<g>148</g>
			<b>255</b>
		</outlineColor>
		<status>true</status>
		<transform>true</transform>
	</gisbeans:layer>
</gisbeans:map>
